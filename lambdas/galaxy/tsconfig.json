{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "lib/*": ["./lib/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
