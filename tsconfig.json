{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "bot": ["./bot"],
      "bot/*": ["./bot/*"],
      "libs": ["./libs"],
      "libs/*": ["./libs/*"],
      "common": ["./common"],
      "common/*": ["./common/*"],
      "parsers": ["./parsers"],
      "parsers/*": ["./parsers/*"],
      "utils": ["./utils"],
      "utils/*": ["./utils/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
